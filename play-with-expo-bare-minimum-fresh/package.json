    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.10.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@types/jest": "27.0.3",
    "@types/react": "17.0.21",
    "@types/react-native": "0.64.12",
    "jest": "27.4.3",
    "jest-expo": "43.0.1",
